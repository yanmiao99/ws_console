{
  "name": "ws-console",
  "displayName": "vscode_console",
  "description": "快速创建一个 console.log 输出",
  "version": "1.0.3",
  "icon": "images/icon.png",
  "publisher": "yanmiao",
  "repository": {
    "type": "git",
    "url": "https://github.com/yanmiao99/ws_console.git"
  },
  "engines": {
    "vscode": "^1.52.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./extension.js",
  "contributes": {
    "commands": [
      {
        "command": "extension.consoleLog",
        "title": "Console Log"
      }
    ],
    "keybindings": [
      {
        "command": "extension.consoleLog",
        "key": "cmd+enter",
        "when": "editorTextFocus && !editorReadonly"
      },
      {
        "command": "extension.consoleLog",
        "key": "alt+enter",
        "when": "editorTextFocus && !editorReadonly"
      }
    ]
  },
  "scripts": {
    "lint": "eslint .",
    "pretest": "npm run lint",
    "test": "vscode-test",
    "package": "pnpm vsce package --no-dependencies",
    "publish": "pnpm vsce publish --no-dependencies"
  },
  "devDependencies": {
    "@types/mocha": "^10.0.7",
    "@types/node": "20.x",
    "@types/vscode": "^1.52.0",
    "@vscode/test-cli": "^0.0.9",
    "@vscode/test-electron": "^2.4.0",
    "eslint": "^8.57.0",
    "typescript": "^5.4.5",
    "vsce": "^2.15.0"
  }
}
